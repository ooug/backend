{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend for ooug",
  "main": "index.js",
  "repository": "git@github.com:ooug/backend.git",
  "author": "santosh mahto <mahtos480@gmail.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": "12"
  },
  "scripts": {
    "start": "node dist/index.js",
    "local": "cross-env NODE_ENV=local nodemon src/index.ts",
    "build": "yarn prebuild && tsc && yarn copy",
    "prebuild": "rimraf ./dist",
    "copy": "copyfiles -u 1 ./src/public/** ./src/assets/** ./dist/",
    "gcp": "gcloud app deploy",
    "deploy": "yarn build && yarn gcp",
    "create-env": "printenv > .env"
  },
  "nodemonConfig": {
    "watch": "src/*",
    "execMap": {
      "ts": "ts-node"
    },
    "delay": "500"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.1.0",
    "@types/multer": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.2",
    "express": "^4.17.1",
    "firebase": "^7.15.1",
    "mongoose": "^5.9.18",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.4.10",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.0.3",
    "@types/express": "^4.17.6",
    "@types/mongoose": "^5.7.21",
    "@types/morgan": "^1.9.0",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "copyfiles": "^2.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "nodemon": "^2.0.3",
    "ts-node": "^8.10.1",
    "tslib": "^2.0.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.2"
  }
}
